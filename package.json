{
  "name": "myapp",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clean": "rimraf node_modules && rimraf android/.gradle android/app/.cxx android/app/build android/build build",
    "clean:android": "cd android && gradlew.bat clean",
    "generate-codegen": "cd android && gradlew.bat generateCodegenArtifactsFromSchema",
    "prepare-android": "node ./node_modules/react-native-reanimated/scripts/prepare-android.js || echo \"no prepare script (ok)\"",
    "build:android": "npm run generate-codegen && npm run prepare-android && cd android && gradlew.bat clean && gradlew.bat assembleRelease --stacktrace --info"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.3.26",
    "@supabase/supabase-js": "^2.57.4",
    "axios": "^1.6.8",
    "cross-fetch": "^4.1.0",
    "expo": "~53.0.0",
    "expo-application": "~6.1.5",
    "expo-asset": "~11.1.7",
    "expo-auth-session": "^6.1.0",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.2",
    "expo-status-bar": "~2.2.3",
    "expo-web-browser": "~14.2.0",
    "form-data": "^4.0.4",
    "i18n-iso-countries": "^7.14.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-iap": "^13.0.4",
    "react-native-reanimated": "3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-svg-transformer": "1.5.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.20.0",
    "strip-ansi": "6.0.1"
  },
  "devDependencies": {
    "@react-native-community/cli": "^13.6.7",
    "@react-native/codegen": "^0.79.5",
    "@react-native/gradle-plugin": "^0.81.1",
    "@types/react": "~19.0.10",
    "metro-react-native-babel-preset": "^0.77.0",
    "typescript": "~5.8.3"
  }
}
